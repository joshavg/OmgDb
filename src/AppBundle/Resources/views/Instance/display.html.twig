{% for p in instance.properties %}
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">{{ p.attribute.name }}</h3>
        </div>
        <div class="panel-body">
            {% if p|escapeproperty %}
                {{ p|propertyvalue }}
            {% else %}
                {{ p|propertyvalue|raw }}
            {% endif %}
        </div>
    </div>
{% endfor %}
